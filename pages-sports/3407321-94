  <script type="text/javascript">
      
      amzn.sx.utils.jsDepMgr.when('jQuery search-js-general breadcrumb', 'searchJsBtf', function($, searchComponents, breadcrumb) {
          
          SPUtils.triggerEvent("spFold");

          
          breadcrumb.breadcrumbSearch();

          
