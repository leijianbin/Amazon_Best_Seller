            registerAivHandler("atf");

              
              setFinancialValidation();
          });

          
          if (typeof processBundlesFlyouts == 'function') {
              processBundlesFlyouts('atf');
          }

          
                      amzn.sx.utils.jsDepMgr.when('popover', 'promotionPopover', function() {
                          processPromotionsPopover();
                      });
          

      });

      </script>

</div>
